<AnchorPane prefHeight="650.0" prefWidth="1100.0" stylesheets="@../styles/appHome.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.o4codes.controllers.AppHomeController">
